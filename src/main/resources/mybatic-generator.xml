<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- <classPathEntry location="./mybatis_gen/postgresql-9.4-1201-jdbc41.jar" />  -->
	<!-- tmnf_drive_agent DB generate definition for pioneer-tmnf-app project. -->
	<context id="spring-primefaces">
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/exercise?useUnicode=yes&amp;characterEncoding=UTF-8"
			userId="root" password="" >
		<property name="nullCatalogMeansCurrent" value="true" />
		</jdbcConnection>
		<javaModelGenerator targetPackage="com.mvc.entity" targetProject=".\src\main\java\" />
		<sqlMapGenerator targetPackage="com.mvc.mapper" targetProject=".\src\main\resources\" />
		<javaClientGenerator targetPackage="com.mvc.mapper" targetProject=".\src\main\java" type="XMLMAPPER" />
		<table schema="exercise" tableName="%" />
	</context>
</generatorConfiguration>
